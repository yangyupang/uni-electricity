<block wx:if="{{dataList}}"><view class="index-container"><view class="index-header flex jc-between a-center"><view data-event-opts="{{[['tap',[['openMap',['$event']]]]]}}" bindtap="__e"><mp-location class="vue-ref" vue-id="8dd740cc-1" data-ref="location" bind:__l="__l"></mp-location></view><view data-event-opts="{{[['tap',[['goto',['/pages/search/search']]]]]}}" class="position-re" bindtap="__e"><input class="header-input" disabled placeholder="请输入关键字" type="text" value=""/><image class="search-pic position-ab" src="../../static/images/search.png" mode></image></view></view><view class="index-swiper"><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"><block wx:for="{{dataList.banner}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><swiper-item><image class="swiper-pic" src="{{item.image_url}}"></image></swiper-item></block></swiper></view><view class="flex jc-around index-channel"><block wx:for="{{dataList.channel}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['goto',['$0'],[[['dataList.channel','id',item.id,'url']]]]]]]}}" class="t-center" bindtap="__e"><image class="channel-pic" src="{{'../../static/images/channel'+(index+1)+'.png'}}" mode></image><view class="t-center">{{''+item.name+''}}</view></view></block></view><view><view class="t-center index-title">品牌制造商直供</view><view class=" flex flex-wrap jc-around"><block wx:for="{{dataList.brandList}}" wx:for-item="item" wx:for-index="__i1__" wx:key="id"><view data-event-opts="{{[['tap',[['$gotoDetail',['brandDetail','$0'],[[['dataList.brandList','id',item.id,'id']]]]]]]}}" class="index-brand position-re" bindtap="__e"><view class="position-ab brand-info" style="z-index:1;"><view>{{''+item.name+''}}</view><view class="brand-price">{{'￥'+item.floor_price+'元起'}}</view></view><image src="{{item.new_pic_url}}" mode></image></view></block></view></view><view><index-goods vue-id="8dd740cc-2" title="新品首发" bgcolor="{{bgcolor.new}}" type="New" items="{{dataList.newGoods}}" bind:__l="__l"></index-goods><index-goods vue-id="8dd740cc-3" title="人气推荐·好物精选" bgcolor="{{bgcolor.hot}}" type="Hot" items="{{dataList.hotGoods}}" bind:__l="__l"></index-goods></view><view><view class="t-center index-title">专题精选</view><scroll-view scroll-x="true"><view class="flex"><block wx:for="{{dataList.topicList}}" wx:for-item="item" wx:for-index="__i2__" wx:key="id"><view data-event-opts="{{[['tap',[['$gotoDetail',['projectDetail','$0'],[[['dataList.topicList','id',item.id,'id']]]]]]]}}" class="topic-box" bindtap="__e"><image src="{{item.scene_pic_url}}" mode></image><view><text>{{item.title}}</text><text style="color:red;margin-left:20rpx;font-size:24rpx;">{{"￥"+item.price_info+"元起"}}</text></view><view class="txt-hidden topic-subtitle">{{item.subtitle}}</view></view></block></view></scroll-view></view><view style="background:#eeeeee;"><block wx:for="{{dataList.newCategoryList}}" wx:for-item="item" wx:for-index="__i3__" wx:key="id"><index-good-box vue-id="{{'8dd740cc-4-'+__i3__}}" item="{{item}}" bind:__l="__l"></index-good-box></block></view></view></block>